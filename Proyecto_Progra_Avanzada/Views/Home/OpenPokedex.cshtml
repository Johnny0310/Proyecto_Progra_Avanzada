@{
}
<body>
    <h1>Lista de Pokemones</h1>

    <table class="table">
        <thead>
            <tr>
                <th>ID Pokedex</th>
                <th>User ID</th>
                <th>PokemonID</th>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var pokedex in Model)
            {
                <tr>
                    <td>@pokedex.id_Pokedex</td>
                    <td>@pokedex.Id</td>
                    <td>@pokedex.PokemonID</td>
                    <td>@pokedex.fecha_captura</td>
                </tr>
            }
        </tbody>
    </table>

    <button type="button" onclick="location.href='@Url.Action("MainView", "Home")'" class="btn btn-primary">Go Back</button>
</body>